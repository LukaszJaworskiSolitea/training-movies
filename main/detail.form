    <ActionBlock>
      <Name value="init_detail_form" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[

//initial settings
form.type=enums.type.movie
form.search_by_actor=defaultFromValue(form.search_by_actor)


apiobjects.breadcrumbsCtrl1.removeAll()
apiobjects.breadcrumbsCtrl1.addBreadcrumb("list","Home","","",true)
if hasValue(form.name) then
	apiobjects.breadcrumbsCtrl1.addBreadcrumb("detail",form.name,"","",false)
endif	

var url=apiobjects.urlRouter1.findFormUrl(navigation.main.detail.fullName)
if hasValue(form.id) then
	url.addString("id",form.id)
endif
url.updatePageUrl()


form.actions.get_detail()
form.actions.activate_detail_form()





]]></Script>
