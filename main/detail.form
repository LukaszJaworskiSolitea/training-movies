    </ActionBlock>
    <ActionBlock>
      <Name value="get_detail" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[

var response = apiobjects.main_server_connection.getAsync("record?id="+form.id)

if response.isError() then 
	system.console(response.getErrorMessage())
else 
	form.detailedMovie.fromJson(response.getJsonString())	
endif
]]></Script>
