    <ActionBlock>
      <Name value="get_detail" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[
globals.busy=true

var response = apiobjects.main_server_connection.getAsync("record?id="+form.id)

if response.isError() then 
	system.console(response.getErrorMessage())
else 
	form.detailedMovie.fromJson(response.getJsonString())	
	form.type=form.detailedMovie.type
endif

globals.busy=false



//update and delete enab/disab
if hasValue(form.detailedMovie.id) then 
	form.helpers._update_enable=true
	form.helpers._del_record_enable=true
	form.delete_id.id=form.detailedMovie.id
else 
	form.helpers._update_enable=false
	form.helpers._del_record_enable=false
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_button1_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[
var validation=true
//check if movie exist 
var already_exist=form.all_data.exists(current.name=form.detailedMovie.name)
if isFalse(already_exist) then
	if form.type=enums.type.movie then 
		//movie validation
		if form.isValid() then 
			form.actions.create_new_record()
		else 
			system.alert("Form is not valid")
			form.setTouched(true)
		endif
	else 
		//series validation:
		//if there is at least one season present	
		if (form.detailedMovie.seasons.count()=0) and (hasValue(form.type)) then
			validation=false
			system.alert("At least one season in series has to be present ")
		else 	
			//make sure every seasons has at leasta one episode...
			foreach season in form.detailedMovie.seasons do 
				if not (season.episodes.count()>0) then
					validation=false
				endif	
			endfor 
			//...and form is valid
			if (form.isValid()) and isTrue(validation) then 
				form.actions.create_new_record()
			else 
				system.alert("Form is not valid")
				form.setTouched(true)
			endif
		endif
	endif
else
	system.alert("That title already exist")
endif


]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_button2_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[
var validation=true
if form.type=enums.type.movie then 
	//movie validation
	if form.isValid() then 
		form.actions.update_record()
	else 
		system.alert("Form is not valid ")
	endif
else 
	//series validation:
	//make sure every seasons has at leasta one episode...
	foreach season in form.detailedMovie.seasons do 
		if not (season.episodes.count()>0) then
			validation=false
		endif	
	endfor 
	
	//...and form is valid
	if (form.isValid()) and isTrue(validation) then 
		form.actions.update_record()
	else 
		system.alert("Form is not valid")
	endif
endif


]]></Script>
